  "bugs": {
    "url": "https://github.com/lctech-jasper/ui-utility/issues"
  },
  "homepage": "https://github.com/lctech-jasper/ui-utility#readme",
  "devDependencies": {
    "@types/node": "^15.0.2",
    "lerna": "^4.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "eslint": "^7.26.0",
    "ts-jest": "^26.5.6"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
