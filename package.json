{
  "name": "@lctech-jasper/ui-utility",
  "version": "1.0.0",
  "private": true,
  "description": "test lit for company UI server",
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "CHANGELOG.md",
    "package.json",
    "packages/**/dist/*",
    "packages/**/*.js",
    "packages/**/*.d.ts",
    "packages/**/CHANGELOG.md",
    "packages/**/package.json"
  ],
  "scripts": {
    "postinstall": "lerna bootstrap --ignore-prepublish",
